{
  "name": "oud-intellisense",
  "displayName": "oud-intellisense",
  "description": "OuDia diagram file helper.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.56.0"
  },
  "publisher": "up-tri",
  "author": {
    "name": "up-tri",
    "email": "yaki-shake@up-tri.me"
  },
  "homepage": "https://github.com/up-tri/oud-intellisense",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:oud-intellisense.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "oudia",
        "extensions": [
          ".oud",
          ".oud2"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "oudia",
        "scopeName": "source.oud",
        "path": "./syntaxes/oud.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "oud-intellisense.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.0",
    "eslint": "^9.10.0",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "glob": "^11.0.0",
    "mocha": "^10.7.3",
    "typescript": "^5.6.2",
    "vscode-test": "^1.6.1"
  }
}