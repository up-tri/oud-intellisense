{
  "name": "oud-intellisense",
  "displayName": "oud-intellisense",
  "description": "OuDia diagram file helper.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.56.0"
  },
  "publisher": "up-tri",
  "author": {
    "name": "up-tri",
    "email": "yaki-shake@up-tri.me"
  },
  "homepage": "https://github.com/up-tri/oud-intellisense",
  "categories": ["Other"],
  "activationEvents": ["onCommand:oud-intellisense.helloWorld"],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "oudia",
        "extensions": [".oud", ".oud2"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "oudia",
        "scopeName": "source.oud",
        "path": "./syntaxes/oud.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "oud-intellisense.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc",
    "build": "tsc",
    "build:watch": "tsc -watch",
    "lint": "biome format .",
    "lint:fix": "biome format --write .",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@biomejs/biome": "1.9.0",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.0",
    "@types/vscode": "^1.93.0",
    "babel-jest": "^29.7.0",
    "glob": "^11.0.0",
    "jest": "^29.7.0",
    "typescript": "~5.5.4",
    "vscode-test": "^1.6.1"
  }
}
