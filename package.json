{
  "name": "oud-intellisense",
  "displayName": "oud-intellisense",
  "description": "OuDia diagram file helper.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.56.0"
  },
  "publisher": "up-tri",
  "author": {
    "name": "up-tri",
    "email": "yaki-shake@up-tri.me"
  },
  "homepage": "https://github.com/up-tri/oud-intellisense",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:oud-intellisense.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [{
      "id": "oudia",
      "extensions": [
        ".oud",
        ".oud2"
      ],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "oudia",
      "scopeName": "source.oud",
      "path": "./syntaxes/oud.tmLanguage.json"
    }],
    "commands": [{
      "command": "oud-intellisense.helloWorld",
      "title": "Hello World"
    }]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.56.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}